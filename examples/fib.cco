// Recursion example (fibonacci sequence)
fun fib(n : int) : int {
    if (n <= 0) {
        return 0;
    } else {
        if (n == 1) {
            return n;
        } else {
            return fib(n - 2) + fib(n - 1);
        }
    }
}

fun main() : int {
    var i : int;
    i := 0;
    while(i < 10) {
        print(fib(i));
        i := i + 1;
    }
}
